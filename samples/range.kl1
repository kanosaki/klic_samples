:- module main.

main :- 
    range(1, 3, L), 
    range_rev(1, 3, RL),
    io:outstream([
        print("range"), print(L), nl,
        print("range_rev"), print(RL), nl]).

range(A, B, L) :- A > B | L = [].
range(A, B, L) :- A =< B |
    A1 := A + 1,
    range(A1, B, L1),
    L = [A|L1].

range_rev(A, B, L) :- A > B | L = [].
range_rev(A, B, L) :- A =< B |
    A1 := A + 1,
    range_rev(A1, B, L1),
    L1 = [A|L].
